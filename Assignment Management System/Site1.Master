    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <style type="text/css">
        table,th,td{
            border: 1px solid #86C232;
            border-collapse: collapse;
        }
        .auto-style1 {
            width: 1352px;
        }
        .auto-style2 {
            width: 1355px;
        }
    </style>
    <link href="StyleSheet1.css" rel="stylesheet" />
